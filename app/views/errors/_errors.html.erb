<% if model.errors.any? %>
    <div id="error_explanation">
        <h2><%=pluralize(model.errors.count, "error") %> prevented this item from being saved.</h2>
            <ul>
                <% model.errors.each do |e| %>
                    <li><%= e.full_message %></li>
                <% end %>
            </ul>
    </div>
<% end %> 